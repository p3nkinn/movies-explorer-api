{
  "extends": "airbnb-base",
    "env": {
        "es2021": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "no-underscore-dangle": ["error", { "allow": ["_id"]}],
      "no-console": "off",
      "linebreak-style": 0
    }
}
